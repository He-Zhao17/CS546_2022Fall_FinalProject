<section class="MainPage">
    <h1>This is your Information of resume '{{resume.resumeName}}'</h1>
    <div>
        <table border="1" class="reviewOneResume">
            <tr>
                <td>Resume Name</td>
                <td>{{resume.resumeName}}</td>
            </tr>
            <tr>
                <td>Name: </td>
                <td>{{resume.firstName}} {{resume.lastName}}</td>
            </tr>
            <tr>
                <td>Email: </td>
                <td>{{resume.email}}</td>
            </tr>
            <tr>
                <td>Gender: </td>
                <td>{{resume.gender}}</td>
            </tr>
            <tr>
                <td>City: </td>
                <td>{{resume.city}}</td>
            </tr>
            <tr>
                <td>State: </td>
                <td>{{resume.state}}</td>
            </tr>
            <tr>
                <td>Country: </td>
                <td>{{resume.country}}</td>
            </tr>
            <tr>
                <td>Age: </td>
                <td>{{resume.age}}</td>
            </tr>
            <tr>
                <td>Telephone Number: </td>
                <td>{{resume.phone}}</td>
            </tr>
            <tr>
                <td>Address: </td>
                <td>{{resume.address}}</td>
            </tr>
            <tr>
                <td>Website url: </td>
                <td>{{resume.website}}</td>
            </tr>
            <tr>
                <td>Skills: </td>
                {{#each resume.skills}}
                    <td></td>
                {{/each}}
            </tr>
            <tr>
                <td>Education: </td>
                {{#each resume.edu}}
                <td>
                    <div>school: {{this.schoolInput}}</div>
                    <div>start date: {{this.startDateInput}}</div>
                    <div>current study: {{this.currentStudyInput}}</div>
                    <div>end date: {{this.endDateInput}}</div>
                    <div>gpa: {{this.gpaInput}}</div>
                    <div>degree: {{this.degreeInput}}</div>
                    <div>major: {{this.majorInput}}</div>
                    <div>country: {{this.countryInput}}</div>
                    <div>state: {{this.stateInput}}</div>
                    <div>city: {{this.cityInput}}</div>
                    <div>description: {{this.descriptionInput}}</div>
                </td>
                {{/each}}
            </tr>
            <tr>
                <td>Employment: </td>
                {{#each resume.emp}}
                <td>
                    <div>employer: {{this.employerInput}}</div>
                    <div>job title: {{this.jobTitleInput}}</div>
                    <div>start date: {{this.startDateInput}}</div>
                    <div>current employed: {{this.currentStudyInput}}</div>
                    <div>country: {{this.countryInput}}</div>
                    <div>state: {{this.stateInput}}</div>
                    <div>city: {{this.cityInput}}</div>
                    <div>description: {{this.descriptionInput}}</div>
                </td>

                {{/each}}
            </tr>

            <tr>
                <td>Project: </td>
                {{#each resume.pro}}
                <td>
                    <div>title: {{this.titleInput}}</div>
                    <div>role: {{this.roleInput}}</div>
                    <div>description: {{this.descriptionInput}}</div>
                </td>
                {{/each}}
            </tr>
        </table>
    </div>
    {{#if isApplicant}}
        <a href="/applicant/reviewResumes" id="return"> ‚Üê Back to List</a>
    {{/if}}

</section>